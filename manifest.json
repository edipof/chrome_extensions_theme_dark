{
	"manifest_version": 2,

	"name": "Change to Dark Theme",
	"description": "Sample extension to make anime page dark",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},

	"permissions": ["tabs", "*://*/*"  ],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Change to Dark Theme!"
	}
}